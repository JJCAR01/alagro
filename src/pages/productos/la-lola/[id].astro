---
import Layout from '../../../layouts/base.astro';
import Header from '../../../components/header/header.astro';
import Footer from '../../../components/footer/footer.astro';
import IdProductoLaLola from '@components/productos/lalola/idProductoLaLola.astro';
import { listaProductosLaLola } from '../../../data/productosLaLola';

export function getStaticPaths() {
  return listaProductosLaLola.map((producto) => ({
    params: { id: producto.id.toString() },
    props: { producto }
  }));
}

const { producto } = Astro.props;
---

<Layout>
  <Header />
  <IdProductoLaLola producto={producto} />
  <Footer />
</Layout>

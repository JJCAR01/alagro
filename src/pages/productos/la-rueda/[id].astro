---
import Layout from '../../../layouts/base.astro';
import Header from '../../../components/header/header.astro';
import Footer from '../../../components/footer/footer.astro';
import IdProductoLaRueda from '@components/productos/larueda/idProductoLaRueda.astro';
import { listaProductosLaRueda } from '../../../data/productosLaRueda';

export function getStaticPaths() {
  return listaProductosLaRueda.map((producto) => ({
    params: { id: producto.id.toString() },
    props: { producto }
  }));
}

const { producto } = Astro.props;
---

<Layout>
    <Header />
    <IdProductoLaRueda producto={producto} />
    <Footer />
</Layout>
